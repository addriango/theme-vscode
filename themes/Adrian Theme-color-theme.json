				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#84a8f7",
				// "foreground": "#94ce73"
				"fontStyle": "bold"
			}
		},
		{
			"name": "Block Level Variables",
