{
  "name": "Adrian Dark-Theme",
  "type": "dark",
  "colors": {
    "widget.shadow": "#bce2a6",
    "selection.background": "#070707",
    //sidebar
    "sideBar.background": "#070707",
    "sideBar.border": "#343435",
    "sideBar.foreground": "#dddcdc",
    "sideBarSectionHeader.background": "#070707",
    "sideBarSectionHeader.foreground": "#2d7",
    //editor colors
    "editor.background": "#070707",
    "editor.foreground": "#d4d4d4",
    "editorLineNumber.foreground": "#969797",
    "editor.lineHighlightBackground": "#1e1e20",
    //terminal
    "terminal.background": "#070707",
    //tab
    "tab.activeBackground": "#232323",
    "tab.inactiveBackground": "#070707",
    //activity bar
    "activityBar.foreground": "#2d7",
    "activityBar.inactiveForeground": "#c7bebc",
    //statusBar
    "statusBar.background": "#070707",
    "statusBar.foreground": "#c9c5c4",
    "sideBarTitle.foreground": "#e6e6e6",
    //activity bar
    "activityBar.background": "#070707",
    "editorGroupHeader.tabsBackground": "#070707"
  },
  "tokenColors": [
    {
      "name": "types golang",
      "scope": [
        "keyword.import.go",
        "keyword.package.go",
        "keyword.function.go",
        "keyword.control.switch.js",
        "keyword.control.switch.js",
        "keyword.operator.new.js",
        "keyword.control.import.js.jsx",
        "keyword.control.from.js.jsx",
        "keyword.control.export.js.jsx",
        "keyword.control.default.js.jsx",
        "keyword.control.flow.python",
        "storage.type.class.python",
        "storage.type.function.python",
        "storage.type.numeric.go",
        "storage.type.string.go",
        "constant.language.go",
        "storage.type.byte.go",
        "storage.type.rune.go",
        "keyword.control.go",
        "keyword.type.go",
        "keyword.struct.go",
        "keyword.operator.comparison.go",
        "keyword.var.go",
        "keyword.operator.assignment.go",
        "keyword.interface.go",
        "storage.type.error.go",
        "storage.type.boolean.go",
        "keyword.channel.go",
        "storage.type.built-in.primitive.c",
        "keyword.control.c",
        "keyword.operator.comparison.c",
        "keyword.control.directive.define.c",
        "keyword.operator.c",
        "keyword.control.switch.c",
        "keyword.control.case.c",
        "keyword.control.directive.include.c"
      ],
      "settings": {
        "foreground": "#2d7"
      }
    },
    {
      "name": "variables javascript",
      "scope": [
        "variable.other.readwrite.js",
        "entity.name.function.js",
        "support.function.console.js",
        "variable.other.property.js",
        "variable.other.constant.js",
        "support.function.math.js",
        "support.variable.property.js",
        "support.function.js",
        "support.function.builtin.python",
        "support.function.magic.python",
        "entity.name.function.python",
        "meta.function-call.generic.python",
        "variable.other.declaration.go"
      ],
      "settings": {
        "foreground": "#0092e7"
      }
    },
    {
      "name": "var  javascript",
      "scope": [
        "storage.type.js",
        "storage.type.function.js",
        "keyword.control.conditional.js",
        "keyword.control.flow.js",
        "keyword.control.loop.js",
        "storage.type.class.js",
        "storage.modifier.js"
      ],
      "settings": {
        "foreground": "#2d7"
      }
    },
    {
      "name": "var  javascript",
      "scope": ["meta.object-literal.key.js", "constant.numeric.decimal.c"],
      "settings": {
        "foreground": "#dd946a"
      }
    },
    {
      "name": "main golang",
      "scope": [
        "entity.name.package.go",
        "entity.name.import.go",
        "keyword.other.special-method.dockerfile",
        "variable.other.assignment.go",
        "support.function.go",
        "string.quoted.other.lt-gt.include.c",
        "entity.name.function.c",
        "constant.character.escape.c"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0092e7"
      }
    },
    {
      "name": "old types golang",
      "scope": ["entity.name.type.go"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bec2c4"
      }
    },
    {
      "name": "name functions golang",
      "scope": "entity.name.function.go",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f3f2f2"
      }
    },
    {
      "name": "texto Dockerfile",
      "scope": ["source.dockerfile", "source.go"],
      "settings": {
        "foreground": "#bec2c4"
      }
    },
    {
      "name": "comentarios",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#637f8d"
      }
    },
    {
      "name": "palabras clave del lenguaje",
      "scope": [
        "keyword.import.go",
        "keyword.package.go",
        "keyword.function.go"
      ],
      "settings": {
        "foreground": "#2d7"
      }
    },
    {
      "name": "texto y signo igual jsx",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#d6d8da"
      }
    },
    {
      "name": "tag jsx y html",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#0092e7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "nombre funciones y Methodos",
      "scope": [
        "entity.name.type.class.python",
        "meta.class.python",
        // "source.python",
        "entity.name.type.class.js.jsx",
        "meta.class.js.jsx",
        "new.expr.js.jsx",
        "meta.var.expr.js.jsx",
        "entity.name.type.js.jsx"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",

        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#b8c3c9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "reglas css",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#076297"
      }
    },
    {
      "name": "Atributos jsx y html",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#2d7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Clases",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#9ef078",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#9ef078",
        "fontStyle": "bold"
      }
    },
